{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "api",
            "image": "happyleader/imposc-service",
            "hostname": "api",
            "essential": false,
            "memory": "128"
        },
        {
            "name": "web",
            "image": "happyleader/imposc-ui",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": "3000",
                    "containerPort": "3000"
                }
            ],
            "memory": "128",
            "links": ["api"]
        }
    ]
}